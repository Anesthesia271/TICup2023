<Application x:Class="TICup2023.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/HandyControl;component/Themes/SkinDefault.xaml" />
                <ResourceDictionary Source="pack://application:,,,/HandyControl;component/Themes/Theme.xaml" />
                <!-- Base -->
                <ResourceDictionary Source="Resource/Style/Color.xaml" />
                <ResourceDictionary Source="Resource/Style/Brush.xaml" />
                <ResourceDictionary Source="Resource/Style/Geometry.xaml" />
                <!-- Theme -->
                <ResourceDictionary Source="Resource/Style/Theme/BaseDark.xaml" />
                <!-- Primary -->
                <ResourceDictionary Source="Resource/Style/Primary/Gold.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>